{
  "name": "prisma-exclude",
  "version": "0.0.1",
  "main": "dist/index.js",
  "author": "ajmnz <arnau@ajb.cat>",
  "license": "MIT",
  "description": "Exclude fields from your Prisma queries",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ajmnz/prisma-exclude"
  },
  "keywords": [
    "prisma",
    "prisma exclude",
    "prisma omit"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf -rf dist tsconfig.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "publish": "dotenv release-it --",
    "lint": "eslint --ext=ts src",
    "format": "prettier --write \"./src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@prisma/client": "3.10.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "dotenv-cli": "5.0.0",
    "eslint": "8.10.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "prisma": "3.10.0",
    "release-it": "14.12.5",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "@prisma/client": "^3.10.0",
    "prisma": "^3.10.0"
  }
}
